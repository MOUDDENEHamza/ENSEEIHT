import javax.swing.*;
import java.awt.image.BufferedImage;

/**
 * Dish is an object with name, price and picture.
 *
 * @author Hamza MOUDDENE
 * @version 1.0
 */
public class Dish {

    String name;
    float price;
    JLabel pictureLabel;
    BufferedImage img;
    ImageIcon icon;

    public Dish(String name, float price) {
        this.name = name;
        this.price = price;
    }

    public String getName() {
        return this.name;
    }

    public float getPrice() {
        return this.price;
    }
}
