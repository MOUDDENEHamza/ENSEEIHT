<?xml version="1.0" encoding="UTF-8"?>
<petrinet:PetriNet
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:petrinet="http://petrinet/"
    xsi:schemaLocation="http://petrinet/ PetriNet.ecore">
  <petrinetelement
      xsi:type="petrinet:Place"
      name="p1"
      linkToSource="//@edge.0"/>
  <petrinetelement
      xsi:type="petrinet:Place"
      name="p2"
      linkToCible="//@edge.1"/>
  <petrinetelement
      xsi:type="petrinet:Transition"
      name="tr"
      linkToSource="//@edge.1"
      linkToCible="//@edge.0"/>
  <edge cible="//@PetriNetElement.2"
      source="//@petrinetelement.0"/>
  <edge cible="//@petrinetelement.1"
      source="//@petrinetelement.2"/>
</petrinet:PetriNet>
