<VBox prefHeight="301.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ConsoleController">
   <children>
      <HBox>
         <children>
            <Label contentDisplay="TOP" prefHeight="26" minHeight="26" maxHeight="26100" prefWidth="3000" text="Console" GridPane.columnIndex="0" GridPane.rowIndex="0">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#cleanAction" GridPane.columnIndex="0" GridPane.rowIndex="0">
               <tooltip>
                  <Tooltip text="Effacer la console" />
               </tooltip>
               <graphic>
                  <ImageView  fitHeight="25" fitWidth="25">
                     <image>
                        <Image url="@../icons/clear.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
      <TextArea fx:id="console" prefHeight="3000" />
   </children>
</VBox>